{
  "manifest_version": 2,
  "name": "Frame Demolition",
  "version": "2.0",
  "description": "Enabling route to load abstracted file layer in select sites.",
  "icons": {
    "48": "icons/icon-48.png",
    "48": "icons/icon-96.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/icon-48.png",
    "default_title": "Accessible support",
    "default_popup": "shortcut_popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.dewey.petra.ac.id/*"
      ],
      "js": [
        "node_modules/jquery/dist/jquery.min.js",
        "node_modules/jszip/dist/jszip.min.js",
        "node_modules/jszip-utils/dist/jszip-utils.min.js",
        "./dewey-content-script.js"
      ],
      "css": [
        "node_modules/materialize-css/dist/css/materialize.min.css",
        "./dewey-style.css"
      ]
    }
  ]
}